# description: Test partial/unknown date parsing and date header selection
# group: [read_pgn]

require chess

query TT
SELECT
  Event,
  strftime(UTCDate, '%Y-%m-%d')
FROM read_pgn('test/pgn_files/partial_dates.pgn')
ORDER BY Event;
----
A Unknown	NULL
B Year Only	2000-01-01
C Year Month	2000-06-01
D Prefer EventDate	1951-09-01
E Prefer Complete Later	1999-12-31
